<?xml version="1.0" encoding="UTF-8"?>
<!-- only ways which match to one of the following key-values pares will be imported -->
<!-- the lines are evaluated in the order of their priority and id (ascending) -->
<!-- only the tag-id of the first match is stored in the table -->
<configuration>
  <!-- Bike infrastructure tags: These will create entries for roads WITH bike lanes -->
  <tag_name name="cycleway" id="1">
    <tag_value name="track"          priority="0.8" id="101" />
    <tag_value name="lane"           priority="1.2" id="102" />
    <tag_value name="opposite_lane"  priority="1.2" id="103" />
    <tag_value name="opposite"       priority="1.2" id="104" />
    <tag_value name="shared_lane"    priority="1.4" id="105" />
    <tag_value name="shared"         priority="1.4" id="106" />
    <tag_value name="share_busway"   priority="1.5" id="107" />
  </tag_name>
  
  <tag_name name="cycleway:right" id="2">
    <tag_value name="track"          priority="0.8" id="201" />
    <tag_value name="lane"           priority="1.2" id="202" />
    <tag_value name="opposite_lane"  priority="1.2" id="203" />
    <tag_value name="opposite"       priority="1.2" id="204" />
    <tag_value name="shared_lane"    priority="1.4" id="205" />
    <tag_value name="shared"         priority="1.4" id="206" />
    <tag_value name="share_busway"   priority="1.5" id="207" />
  </tag_name>
  
  <tag_name name="cycleway:left" id="3">
    <tag_value name="track"          priority="0.8" id="301" />
    <tag_value name="lane"           priority="1.2" id="302" />
    <tag_value name="opposite_lane"  priority="1.2" id="303" />
    <tag_value name="opposite"       priority="1.2" id="304" />
    <tag_value name="shared_lane"    priority="1.4" id="305" />
    <tag_value name="shared"         priority="1.4" id="306" />
    <tag_value name="share_busway"   priority="1.5" id="307" />
  </tag_name>
  
  <!-- Surface quality for unpaved paths/tracks -->
  <tag_name name="tracktype" id="4">
    <tag_value name="grade1"         priority="1.0" id="401" />
    <tag_value name="grade2"         priority="1.2" id="402" />
    <tag_value name="grade3"         priority="1.5" id="403" />
    <tag_value name="grade4"         priority="2.0" id="404" />
    <tag_value name="grade5"         priority="3.0" id="405" />
  </tag_name>
  
  <!-- Main highway classification -->
  <tag_name name="highway" id="5">
    <!-- BEST: Dedicated bike infrastructure -->
    <tag_value name="cycleway"       priority="1.0" id="501" />
    <tag_value name="path"           priority="1.1" id="502" />
    
    <!-- GOOD: Low-traffic residential -->
    <tag_value name="living_street"  priority="1.5" id="507" />
    <tag_value name="residential"    priority="2.0" id="509" />
    <tag_value name="service"        priority="2.0" id="508" />
    
    <!-- OK: Minor roads -->
    <tag_value name="unclassified"   priority="2.3" id="510" />
    <tag_value name="track"          priority="2.5" id="506" />
    <tag_value name="tertiary"       priority="2.8" id="512" />
    <tag_value name="tertiary_link"  priority="2.8" id="511" />
    
    <!-- ACCEPTABLE: Bigger roads -->
    <tag_value name="secondary"      priority="3.5" id="515" />
    <tag_value name="secondary_link" priority="3.5" id="514" />
    <tag_value name="primary"        priority="4.5" id="517" />
    <tag_value name="primary_link"   priority="4.5" id="516" />
    
    <!-- AVOID: Major roads -->
    <tag_value name="trunk"          priority="8.0" id="518" />
    <tag_value name="trunk_link"     priority="8.0" id="519" />
    
    <!-- Pedestrian areas (sometimes useful) -->
    <tag_value name="pedestrian"     priority="5.0" id="503" />
    <tag_value name="footway"        priority="6.0" id="504" />
    <tag_value name="bridleway"      priority="5.0" id="505" />
    <tag_value name="steps"          priority="20.0" id="520" />
    
    <!-- Catch-all -->
    <tag_value name="road"           priority="4.0" id="500" />
  </tag_name>
  
  <!-- Roundabouts -->
  <tag_name name="junction" id="6">
    <tag_value name="roundabout"     priority="1.0" id="601" />
  </tag_name>
</configuration>